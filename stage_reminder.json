    {
      "parameters": {
        "jsCode": "const { judgeList, latestWeiboTime, targetList, newTargetList } = $input.first().json\n\nconst targetStageMap = new Map()\nnewTargetList.forEach(newTarget => {\n  targetStageMap.set(newTarget.weibo_uid, {\n    name: newTarget.name,\n    weibo_uid: newTarget.weibo_uid,\n    maxTime: newTarget.maxTime,\n    stages: []\n  })\n})\n\nfor (const judge of judgeList) {\n  if (!judge.message.content.found || !targetStageMap.has(judge.weibo_uid)) continue\n\n  const targetStage = targetStageMap.get(judge.weibo_uid)\n  const stage = judge.message.content.stage\n  // Ê∑ªÂä†ÂæÆÂçöÈìæÊé•\n  stage.weibo_url = `https://m.weibo.cn/detail/${judge.weiboList.id}`\n  // ÁâπÊÆäÂ§ÑÁêÜ‚Äú‰ªäÊó•‚Äù\n  if (stage.ticket_time === '‰ªäÊó•') {\n    const d = new Date(judge.weiboList.time)\n    stage.ticket_time = `${d.getFullYear()}Âπ¥${d.getMonth()+1}Êúà${d.getDate()}Êó•`\n  }\n  targetStage.stages.push(stage)\n}\n\nlet hasStage = false\nfor (const targetStage of targetStageMap.values()) {\n  if (targetStage.stages.length > 0) {\n    hasStage = true\n    break\n  }\n}\n\nreturn [{\n  json: {\n    latestWeiboTime,\n    targetList,\n    targetStageList: Array.from(targetStageMap.values()),\n    hasStage\n  }\n}]"
      },
